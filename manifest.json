{

    "manifest_version": 3,
    "name": "QuickSkip: YouTube Ad Blocker",
    "version": "1.0.1",
    "description": "Cut down .",

    "content_scripts": [
        {
        "matches": ["https://www.youtube.com/*"],
        "js": ["skip.js"]
        }
    ],

    "web_accessible_resources": [
        {
        "matches": ["<all_urls>"],
        "resources": ["*", "**/*"]
        }
    ]
}